{
  "name": "vue-dnd-hooks",
  "version": "1.1.2",
  "private": false,
  "description": "Vue 3 Drag and Drop Hooks Library",
  "author": "ZIZIGY",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "vue",
    "drag-and-drop",
    "hooks",
    "typescript"
  ],
  "main": "./dist/vue-dnd-hooks.umd.js",
  "module": "./dist/vue-dnd-hooks.es.js",
