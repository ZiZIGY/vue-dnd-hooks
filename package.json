{
  "name": "vue-dnd-hooks",
  "version": "1.2.6",
  "private": false,
  "description": "Vue 3 Drag and Drop Hooks Library",
  "author": "ZIZIGY",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "vue",
    "drag-and-drop",
    "dnd",
    "typescript-library",
    "draggable",
    "droppable",
    "webdev",
    "vue3",
    "vue-dnd",
    "vue-dnd-hooks",
    "vue-hooks",
    "vue-composable",
    "frontend",
    "dnd-kit",
    "hooks",
    "typescript"
  ],
  "main": "./lib/vue-dnd-hooks.umd.js",
  "module": "./lib/vue-dnd-hooks.es.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "dev": "vite --host",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:docs": "vite build --config vite.config.docs.ts",
    "prepare": "npm run build:lib"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/vue-dnd-hooks.es.js",
      "require": "./lib/vue-dnd-hooks.umd.js"
    },
    "./style.css": "./lib/style.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {}
}
